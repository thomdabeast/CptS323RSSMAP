<Window x:Class="Button_Crushers_RSS_Client.HelperWindows.NameChannelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Button_Crushers_RSS_Client"
        mc:Ignorable="d"
        Title="Name Channel" Height="66" Width="296" WindowStyle="ToolWindow" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded">
    <Grid VerticalAlignment="Top">
        <StackPanel Orientation="Horizontal">
            <TextBox x:FieldModifier="private" x:Name="NameTextBox" Height="20" Width="200" Margin="3"/>
            <Button x:FieldModifier="private" x:Name="AddButton" Height="20" Margin="3" Click="Add_Click">add</Button>
            <Button x:FieldModifier="private" x:Name="CancelButton" Height="20" Margin="3" Click="Cancel_Click">cancel</Button>
        </StackPanel>
    </Grid>
</Window>
