<Window x:Class="Button_Crushers_RSS_Client.HelperWindows.RenameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Button_Crushers_RSS_Client.HelperWindows"
        mc:Ignorable="d"
        Title="RenameWindow" Height="161.925" Width="300">
    <Grid Margin="0,0,0,26">
        <StackPanel Orientation="Horizontal">
            <TextBox x:FieldModifier="private" x:Name="NameTextBox" Height="20" Width="180" Margin="3"/>
            <Button x:FieldModifier="private" x:Name="RenameButton" Height="20" Margin="3" Click="RenameButton_OnClick">Rename</Button>
            <Button x:FieldModifier="private" x:Name="CancelButton" Height="20" Margin="3" Click="CancelButton_OnClick">Cancel</Button>
        </StackPanel>
    </Grid>
</Window>
